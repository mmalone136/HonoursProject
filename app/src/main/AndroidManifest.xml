<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.mmalo.prototype2">
    <uses-permission android:name="android.permission.CAMERA" />
    <uses-feature android:name="android.hardware.camera" />
    <uses-feature android:name="android.hardware.camera.autofocus"/>
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".LoginActivity">

        </activity>
        <activity android:name=".GuideActivity"> </activity>

        <activity android:name=".MainActivity"> </activity>
        <activity android:name=".PTakenActivity" android:screenOrientation="portrait"> </activity>
        <activity android:name=".DateviewActivity" android:screenOrientation="portrait"> </activity>
        <activity android:name=".OptionsActivity">


            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".EditEntryActivity"> </activity>
        <activity android:name=".SummaryActivity" android:screenOrientation="portrait"> </activity>
        <activity android:name=".SumOptions" android:screenOrientation="portrait"> </activity>

        <activity
            android:name=".CameraActivity"
            android:label="@string/app_name"
            android:screenOrientation="portrait">
        </activity>


    </application>

</manifest>